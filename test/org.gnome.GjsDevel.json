{
    "id": "org.gnome.GjsDevel",
    "branch": "master",
    "runtime": "org.gnome.Platform",
    "runtime-version": "master",
    "sdk": "org.gnome.Sdk",
    "command": "gjs-console",
    "tags": ["unstable", "development"],
    "finish-args": [
        "--filesystem=host",
        "--share=ipc",
        "--share=network",
        "--device=dri",
        "--socket=x11",
        "--socket=wayland",
        "--socket=pulseaudio"
    ],
    "modules": [
        {
            "name": "sysprof",
            "builddir": true,
            "buildsystem": "meson",
            "config-opts": [
                "-Denable_gtk=false",
                "-Dhelp=false",
                "-Dlibsysprof=false",
                "-Dwith_sysprofd=none"
            ],
            "sources": [
                {
                    "type": "git",
                    "branch": "master",
                    "url": "https://gitlab.gnome.org/GNOME/sysprof.git"
                }
            ]
        },
        {
            "name": "gjs",
            "builddir": true,
            "sources": [
                {
                    "type": "git",
                    "branch": "HEAD",
                    "path": "."
                }
            ]
        }
    ]
}
